zend_extension=yasd.so
yasd.debug_mode=remote
yasd.remote_host=host.docker.internal
yasd.remote_port=9000

; By having option "yasd.log_level" set to 0, we can add debugging information to file "debug.log" under the same
; directory of the running PHP script.
; If you want to log debugging information, uncomment next line please.
; yasd.log_level=0
